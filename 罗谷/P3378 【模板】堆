#include<bits/stdc++.h>
#define ll long long
using namespace std;
priority_queue<int,vector<int>,greater<int> > q;
int main(){
    int qu_es;
    cin>>qu_es;
    for (int i=1;i<=qu_es;i++){
        int x;
        cin>>x;
        if (x==1){
            int k;
            cin>>k;
            q.push(k);
        }
        else {
            if (x==2){
                cout<<q.top()<<endl;
            }
            else {
                q.pop();
            }
        }
    }
    return 0;
}
